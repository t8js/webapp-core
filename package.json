{
  "name": "@t8/webapp-core",
  "version": "0.1.7",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx npm-run-all clean compile",
    "clean": "node -e \"require('node:fs').rmSync('dist', { force: true, recursive: true });\"",
    "compile": "npx esbuild index.ts --bundle --outdir=dist --platform=node --external:express",
    "prepublishOnly": "npm run build",
    "preversion": "npx npm-run-all typecheck shape build",
    "shape": "npx codeshape",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t8js/webapp-core.git"
  },
  "author": "axtk",
  "license": "MIT",
  "peerDependencies": {
    "express": ">=5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@t8/date-format": "^1.0.9"
  }
}
